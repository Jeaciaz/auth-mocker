{
    "extends": ["stylelint-config-standard"],
    "plugins": [
        "stylelint-media-use-custom-media"
    ],
    "rules": {
        "csstools/media-use-custom-media": "always",
        "at-rule-no-unknown": [ true, {
            "ignoreAtRules": [
                "custom-media"
            ]
        }]
    }
}
